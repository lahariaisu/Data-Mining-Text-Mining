18430 +/*
18431 +  Macro: tmpred(inds,outds,clusters,type=,ignore=,freqDS=)
18432 +
18433 +  Description: This macro will take output from
18434 +     emclus and generate predicted
18435 +      predicted clusters.
18436 +
18437 +  Parms: inds - input data set, produced by emclus.
18438 +   outds - data set with clusters created by this macro.
18439 +   clusters - number of clusters that should be on this data set
18440 +   type - valid values: 'max' or 'prob'.  Max will just take the
18441 +      cluster with the highest probability as the predicted cluster.
18442 +      In the case of a tie the first cluster with that value will
18443 +       selected.
18444 +      Prob will attempt to select multiple clusters based on
18445 +      how many clusters were requested.  That is if 3 clusters
18446 +      were requested, any cluster with prob>1/3 will be selected
18447 +      as the predicted cluster. THIS OPTION IS UNTESTED.
18448 +*/
18449 +
18450 +%macro tmpred(inds,outds,clusters,type=, ignore=0, freqDS=, compress=1);
18451 +
18452 +  %local _str newfreqDS _flag;
18453 +
18454 +  * set the optional arguments;
18455 +  %if &type= %then %let type=max;
18456 +  %if &ignore = 1 %then %let
18457 +    _str = if _CLUSTER_ = &clusters then _CLUSTER_ = .;
18458 +  %else %let _str = ;
18459 +  %if %str(&freqDS) ^= %str() %then %do;
18460 +   %let newfreqDS = &freqDS(keep=_c1-_c&clusters);
18461 +   %let _flag = 1;
18462 +  %end;
18463 +  %else %let _flag = 0;
18464 +
18465 +  %if &compress = 1 %then %let compress = compress=Y;
18466 +  %else %let compress = ;
18467 +
18468 +  data &outds(&compress drop=_c1-_c&clusters)
18469 +    %if &_flag = 1 %then %do;
18470 +    &newfreqDS
18471 +    %end;
18472 +    ;
18473 +    set &inds end=eof;
18474 +   * array for holding all probabilities;
18475 +    array _p prob1--prob&clusters;
18476 +    array _c(&clusters);
18477 +    retain _c 0;
18478 +   * get rid of working columns;
18479 +    drop _max_ _i /*prob1--prob&clusters*/ _WARN_;
18480 +    /* keep the largest probability and the cluster */
18481 +    _max_ = 0;
18482 +    do _i = 1 to &clusters;
18483 +      if _p(_i) > _max_ then do;
18484 +        _max_ = _p(_i);
18485 +        _CLUSTER_ = _i;
18486 +      end;
18487 +    end;
18488 +    if (_CLUSTER_ >= 1) and (_CLUSTER_ <= &clusters) then
18489 +      _c(_CLUSTER_) = _c(_CLUSTER_) + 1;
18490 +    else _CLUSTER_ = .;
18491 +    &_str;
18492 +    %if &_flag = 1 %then %do;
18493 +      if eof then do;
18494 +        output &freqDS;
18495 +                output &outDS;
18496 +          end;
18497 +          else output &outDS;
18498 +        %end;
18499 +
18500 +    run;
18501 +%mend tmpred;
